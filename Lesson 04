// Query The DOM
const wrap = document.querySelector('#book-list li:nth-child(2).name'); //returns single element
// console.log(wrap);

let books = document.querySelector('#book-list li .name');
// console.log(books);


books = document.querySelectorAll('#book-list li .name'); //return collection of elements
// console.log(books);

Array.from(books).forEach(function(book) {
    console.log(book);
})
